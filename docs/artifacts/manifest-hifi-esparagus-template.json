{
    "name": "snapclient on the hifi-esparagus",
    "version": "latest",
    "home_assistant_domain": "media_player",
    "funding_url": "https://esphome.io/guides/supporters.html",
    "new_install_prompt_erase": true,
    "new_install_improv_wait_time": 20,
    "builds": [
        {
            "chipFamily": "ESP32",
            "parts": [
                {
                    "path": "hifi-snapclient-latest-bootloader.bin",
                    "offset": "0x1000"
                },
                {
                    "path": "hifi-snapclient-latest-partition-table.bin",
                    "offset": "0x8000"
                },
                {
                    "path": "hifi-snapclient-latest-ota-data-initial.bin",
                    "offset": "0xd000"
                },
                {
                    "path": "hifi-snapclient-latest-snapclient.bin",
                    "offset": "0x10000"
                },
                {
                    "path": "hifi-snapclient-latest-storage.bin",
                    "offset": "0x370000"
                }
            ]
        }
    ],
    "manifest_name": "manifest-hifi-esparagus-latest.json"
}